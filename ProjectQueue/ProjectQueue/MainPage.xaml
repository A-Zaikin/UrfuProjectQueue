<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ProjectQueue.MainPage">

    <StackLayout>
        <Frame BackgroundColor="#2196F3" Padding="24" CornerRadius="0">
            <Label Text="Welcome to Xamarin.Forms!" HorizontalTextAlignment="Center" TextColor="White" FontSize="36"/>
        </Frame>
        <Label Text="{Binding DebugLabel}"></Label>
        <Entry 
            x:Name="entry"
            Text="https://docs.google.com/spreadsheets/d/1LZU6GiXYLOFSbL5ZqImKqNYzYhWiueqONTh__dop4zI/edit#gid=2095115966"
            Completed="EntryCompleted"/>
        <Picker
            x:Name="teamPicker"
            SelectedIndexChanged="TeamPickerSelectedIndexChanged"/>

        <Button 
            Text="Unsubscribe"
            Pressed="UnsubscribeButtonPressed"/>
    </StackLayout>

</ContentPage>
